        "enzyme-adapter-react-16": "^1.15.6",
        "eslint-plugin-mocha": "^7.0.0",
        "eslint-plugin-react": "^7.24.0",
        "path": "^0.12.7",
        "ts-loader": "^8.0.1",
        "ts-mocha": "^8.0.0",
        "typescript": "^4.5.4",
        "webpack": "^4.41.5",
        "webpack-cli": "^3.3.10"
