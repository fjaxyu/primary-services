{
    "name": "primary-services",
    "version": "1.0.2",
    "description": "A Typescript utility service library",
    "main": "./dist/index.js",
    "type": "module",
    "types": "./dist/index.d.ts",
    "scripts": {
        "start": "cd playground && npm start",
        "test": "npm run test-services",
        "test-services": "ts-mocha -p tsconfig.json src/components/**/*.spec.ts",
        "build": "npm run build-services",
        "build-services": "webpack --display-modules --config webpack.config.cjs",
        "build-patch-publish": "npm run build && npm version patch && npm publish",
        "prepublishOnly": "npm run test"
    },
    "keywords": [
        "utility",
        "services",
        "ajax"
    ],
    "peerDependencies": {},
    "dependencies": {
        "@babel/plugin-transform-runtime": "^7.13.7",
        "node-fetch": "^2.6.0",
        "unfetch": "^4.1.0"
    },
    "devDependencies": {
        "@babel/core": "^7.8.3",
        "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
        "@babel/plugin-transform-modules-commonjs": "^7.8.3",
        "@babel/plugin-transform-react-jsx": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.9.6",
        "@babel/preset-env": "^7.8.7",
        "@babel/preset-react": "^7.8.7",
        "@babel/register": "^7.8.3",
        "@babel/runtime": "^7.9.6",
        "@types/expect": "^24.3.0",
        "@types/mocha": "^9.0.0",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-loader": "^8.0.6",
